	.title	OPP_HARDRET

	g_msf


	$linkage_section
a_frame_pointer:
	.address  frame_pointer

	$routine OPP_HARDRET, entry=OPP_HARDRET_CA, kind=null
	lda	sp, -8(sp)
	stq	r3, (sp)
	mov	r27, r3
	.base	r3, $ls

	putframe
	$call	OP_HARDRET, set_arg_info=false, nonstandard=true
	getframe

	ldq	r3, (sp)
	lda	sp, 8(sp)
	ret	r26

	$end_routine

	.end
